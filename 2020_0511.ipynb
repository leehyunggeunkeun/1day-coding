   "source": [
    "#숫자를 입력받으면 그에해당하는 자릿수를 출력하는 코드를 작성하라.\n",
    "#입력 : 156 출력 : 100의자리수\n",
    "#입력 : 18961 출력 : 10000의자릿수"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "숫자를 입력하세용: 18961\n"
     ]
    }
   ],
   "source": [
    "num=input('숫자를 입력하세용: ')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [],
   "source": [
    "def func1(num):\n",
    "    jarisu=len(num)-1\n",
    "\n",
    "    output=10**jarisu\n",
    "    return str(output)+'의 자리수'"
   ]
  },
